{
  "project_type": "node",
  "build_tool": "npm",
  "lint_command": "npm run lint",
  "test_command": "npm test -- --coverage",
  "build_command": "npm run build",
  "artifact_path": "dist/",
  "cache": "npm",
  "node_version": "18",
   "matrix": {
    "node_versions": ["16.x", "18.x", "20.x"]
  },
  "container": {
    "enabled": true,
    "registry": "docker.io",
    "image_name": "myapp",
    "tags": ["latest"],
    "push": true
  }
}
